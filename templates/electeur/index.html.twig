{% extends 'base.html.twig' %}

{% block title %}Electeur index{% endblock %}

{% block body %}
    <h1>Electeur index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Electeur</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for electeur in electeurs %}
            <tr>
                <td>{{ electeur.id }}</td>
                <td>{{ electeur.electeur }}</td>
                <td>
                    <a href="{{ path('electeur_show', {'id': electeur.id}) }}">show</a>
                    <a href="{{ path('electeur_edit', {'id': electeur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('electeur_new') }}">Create new</a>
{% endblock %}
